<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="?attr/colorControlHighlight">
    <item>
        <shape>
            <corners
                android:bottomLeftRadius="?bottomLeftCornerRadius"
                android:bottomRightRadius="?bottomRightCornerRadius"
                android:radius="?cornerRadius"
                android:topLeftRadius="?topLeftCornerRadius"
                android:topRightRadius="?topRightCornerRadius" />
            <stroke
                android:width="?roundedCornerStrokeWidth"
                android:color="?roundedCornerStrokeColor" />
        </shape>
    </item>
</ripple>